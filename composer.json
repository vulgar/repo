
{
    "name": "vulgar/repo",
    "description": "A Laravel package that fetches your GitHub public repositories and saves README files into db for use on web pages",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Matthew Newport",
            "email": "admin@matthewnewport.com",
            "homepage": "https://github.com/vulgar",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "guzzlehttp/guzzle": "^7.3",
        "illuminate/support": "^8.0|^9.0",
        "illuminate/database": "^8.0|^9.0",
        "illuminate/cache": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Vulgar\\Repo\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vulgar\\Repo\\Providers\\RepoServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
